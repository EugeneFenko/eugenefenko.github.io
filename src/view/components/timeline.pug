#timeline.container-fluid.timeline_box.box
  .container
    .page-header
      h2.box_title.center Timeline
    ul.timeline
      li
        .timeline-badge(data-aos='zoom-in')
          i.fa.fa-check
        .timeline-panel(data-aos='fade-up')
          .timeline-heading
            h4.timeline-title
              i.fa.fa-graduation-cap
              |  School
            p
              small.text-muted
                i.far.fa-clock
                |  2003 - 2014
          .timeline-body
            p School graduation
      li.timeline-inverted
        .timeline-badge(data-aos='zoom-in')
          i.fa.fa-check
        .timeline-panel(data-aos='fade-up')
          .timeline-heading
